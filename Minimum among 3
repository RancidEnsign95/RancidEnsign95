#include"stdio.h"
int main()
{
	int a,b,c;
	printf("Enter a value ");scanf("%d",&a);
	printf("Enter another value ");scanf("%d",&b);
	printf("Enter one last value ");scanf("%d",&c);
	if(a<b && a<c)
	printf("%d is the minimum value",a);
	else if(b<a && b<c)
	printf("%d is the minimum value ",b);
	else
	printf("%d is the minimum value ",c);
	return 0;
}
