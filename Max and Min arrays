#include"stdio.h"
int main()
{
    int a[3],i,j,max=0,min=0;
    for(i=1;i<=3;i++)
    {
        printf("Enter [%d] number ",i);scanf("%d",&a[i]);
    }
    for(i=1;i<=3;i++)
    {
    	for(j=1;j<=3;j++)
    	{
        if(a[i]>a[j])
        max=a[i];
        else
        min=a[j];
        }
    }
    printf("Max number is %d",max);
    printf("Min number is %d",min);
    return 0;
}
