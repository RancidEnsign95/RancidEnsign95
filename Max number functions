#include"stdio.h"
int max();
void main()
{
    int c;
    c=max();
    printf("\nMax number is %d",c);
}
int max()
{
    int x,y;
    printf("Enter 2 numbers ");
    scanf("%d%d",&x,&y);
    if(x>y)
    printf("%d is the max number ",x);
    else
    printf("%d is the max number",y);
    return(x,y);
}
