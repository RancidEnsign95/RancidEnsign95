#include"stdio.h"
int main()
{
    char ch;printf("Enter a letter ");scanf("%c",&ch);
    if(ch=='A' && ch=='E' && ch=='I' && ch=='O' && ch=='U')
    printf("Entered letter is a vowel ");
    else if(ch=='a' && ch=='e' && ch=='i' && ch=='o' && ch=='u')
    printf("Entered letter is a vowel");
    else
    printf("Entered letter is a consonent");
    return 0;
}
